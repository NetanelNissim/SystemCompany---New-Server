using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Entities
{
    public class DateProjectCompanies
    {
        public DateProjectCompanies(int idCompany, int idProject, DateTime? DateStartProject, DateTime? DateFinishProject)
        {
            this.idCompany = idCompany;
            this.idProject = idProject;
            this.DateStartProject = DateStartProject;
            this.DateFinishProject = DateFinishProject;
        }

        public DateProjectCompanies(int idCompany, int idProject)
        {
            this.idCompany = idCompany;
            this.idProject = idProject;
        }

        public DateProjectCompanies()
        { }

        public int idProject { get; set; }
        public int idCompany { get; set; }
        public DateTime? DateStartProject { get; set; }
        public DateTime? DateFinishProject { get; set; }
       
    }
}
